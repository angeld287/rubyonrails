
<div class="col-sm-6 col-md-12">
  <ul class="thumbnails">
    <li class="span4" style="float: left; list-style: none;">
      <div class="row">
        <div class="col-sm-6 col-md-6">
          <div class="thumbnail" style="width: 800px; margin: 10px;">
            <div class="caption">
              <h3>Provincia: <%= @propiedades.provincia %></h3>
              <h4>Dimensiones: <%= @propiedades.dimensiones %> Metros Cuadrados</h4>
              <p>Descripcion: <%= @propiedades.descripcion %></p>
              <p>Valoracion: RD$ <%= @propiedades.valoracion %></p>
              <p>Direccion: <%= @propiedades.direccion %></p>
              <p>Ciudad: <%= @propiedades.ciudad %></p>
              <p>Pais: <%= @propiedades.pais %></p>
              <div class="btn-group">
                <button type="button" class="btn btn-default"><%= link_to 'Edit', edit_propiedade_path(@propiedades) %></button>
                <button type="button" class="btn btn-default"><%= link_to 'Back', propiedades_path %></button></button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </li>
  </ul>
</div>